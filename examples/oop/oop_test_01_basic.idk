kelas Manusia {
    nama = kosong
    umur = kosong
    
    fungsi __init__(nama_baru, umur_baru) {
        diri.nama = nama_baru
        diri.umur = umur_baru
    }
    
    fungsi sapa() {
        cetak("Halo, saya adalah", diri.nama)
        cetak("Umur saya:", diri.umur)
    }
}

manusia1 = Manusia("Budi", 25)
manusia1.sapa()
